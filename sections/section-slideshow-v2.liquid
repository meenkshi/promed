<link rel="preconnect" href="https://fonts.googleapis.com">
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
{% for block in section.blocks %}
{% if block.type == "slideshow" %}
  {% if block.settings.fonts_sub_title !=blank %}
  <link href="https://fonts.googleapis.com/css2?family={{block.settings.fonts_sub_title}}:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap" rel="stylesheet">
  {% endif %}
  {% if block.settings.fonts_title !=blank %}
  <link href="https://fonts.googleapis.com/css2?family={{block.settings.fonts_title}}:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap" rel="stylesheet">
  {% endif %}
  {% if block.settings.fonts_sub_title2 !=blank %}
  <link href="https://fonts.googleapis.com/css2?family={{block.settings.fonts_sub_title2}}:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap" rel="stylesheet">
  {% endif %}
  {% if block.settings.fonts_button !=blank %}
  <link href="https://fonts.googleapis.com/css2?family={{block.settings.fonts_button}}:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap" rel="stylesheet">
  {% endif %}
{% endif %}
{% endfor %}

<div data-section-id="{{ section.id }}" data-section-type="section-slideshow-v2" style="{% if section.settings.margin_top != blank %}margin-top: {{section.settings.margin_top}}px;{%endif%} {% if section.settings.margin_bottom != blank %}margin-bottom: {{section.settings.margin_bottom}}px;{% endif %} {% if section.settings.sections_layout == "dark" %}background: {{section.settings.sections_bg_color}};{% endif %}">
  <div class="section-slideshow-v2">
    <div class="slick-side-h2 js-slideshow-v2">       
      {% if section.blocks.size > 0 %}
      {% for block in section.blocks %}
      {% if block.type == "slideshow" %}
      <div class="itemv-slide-h2">
        <div class=" info-sideh2">
          <div class="picture-slideshow">
            <div class="d-none d-md-block">
              {% if block.settings.ena_slide_video %}
              {% if block.settings.url_ytb_video != blank %}
              <div class="over_slidevid"></div>
              {{block.settings.url_ytb_video}}
              {% endif %}
              {% else %}
              {% if block.settings.block_banerimage != blank %}
              <a href="{{block.settings.block_url}}">
                <img data-src="{{block.settings.block_banerimage | img_url: 'master'}}" class="lazyload img-fluid w-100" alt="_img_slide_">
              </a>
              {% else %}
              <a href="{{block.settings.block_url}}">
                <img data-src="//via.placeholder.com/1920x900" class="lazyload img-fluid w-100" alt="_img_slide_">
              </a>
              {% endif %}
              {% endif %}
            </div>
            <div class="d-block d-md-none">
              {% if block.settings.block_banerimage_mb != blank %}
              <a href="{{block.settings.block_url}}">
                <img data-src="{{block.settings.block_banerimage_mb | img_url: 'master'}}" class="lazyload img-fluid w-100" alt="_img_slide_">
              </a>
              {% else %}
              <a href="{{block.settings.block_url}}">
                <img data-src="{{block.settings.block_banerimage | img_url: 'master'}}" class="lazyload img-fluid w-100" alt="_img_slide_">
              </a>
              {% endif %}
            </div>
          </div>
          <div class="box-content {% if block.settings.select_position_content == 'left' %}box-content-left {% elsif block.settings.select_position_content == 'center' %}box-content-center {% elsif block.settings.select_position_content == 'right' %} box-content-right{% endif %}" >
            {% if block.settings.block_sub_title or block.settings.block_title or block.settings.block_sub_title2 or block.settings.block_buton != blank %}
            <div class="{%if settings.enable_rtl_mode %}text-right {% else %}{% if block.settings.select_textalign_content == 'left' %} text-left {% elsif block.settings.select_textalign_content == 'center' %} text-center {% else %} text-right {% endif %}{% endif %}box-info box-info-{{block.id}}">
              <div class="box-sub-title box-sub-title-{{block.id}}  animated">
                {% if block.settings.block_sub_title != blank %}
                <h3 class="title-small mb-0">{{ block.settings.block_sub_title }}</h3>
                {% endif %}
              </div>
              <div class="box-title box-title-{{block.id}} animated">
                {% if block.settings.block_title != blank %}
                <h3 class="titlebig mb-0">{{ block.settings.block_title }}</h3>
                {% endif %} 
              </div>
              <div class="box-sub-title2 box-sub-title2-{{block.id}}  animated">
                {% if block.settings.block_sub_title2 != blank %}
                <h3 class="title-small mb-0">{{ block.settings.block_sub_title2 }}</h3>
                {% endif %}
              </div>
              {% if block.settings.block_buton != blank %}
              <div class="btn-animation-{{block.id}} animated">
                <a class="button-main1 button-shop-{{block.id}}  " href="{{block.settings.block_url}}">
                  {{ block.settings.block_buton }}
                </a>
              </div>
              {% endif %}      
            </div>
            {% endif %}
          </div>
        </div>
      </div>
      {% endif %}
      {% endfor %}
      {% endif %}
    </div>
  </div>
</div>

<style>
  {% if section.blocks.size > 0 %}
  {% for block in section.blocks %}
  {% if block.type == "slideshow" %}
  .section-slideshow-v2 .slick-active .info-sideh2 .box-content .button-shop-{{block.id}}{
    background: linear-gradient(to right,{{block.settings.bg_color_hover_button}} 50%,{{block.settings.bg_color_block_button}} 50%) no-repeat scroll right bottom/210% 100% {{block.settings.bg_color_block_button}};
    color : {{block.settings.color_block_button}};
  }
  .section-slideshow-v2 .slick-active .info-sideh2 .box-content .button-shop-{{block.id}}:hover{
    color : {{block.settings.color_hover_text}};
  }

  .section-slideshow-v2 .slick-active .info-sideh2 .box-info-{{block.id}} .box-sub-title-{{block.id}}{    
    -webkit-animation-name: {{block.settings.effect_sub_title}};
    animation-name:  {{block.settings.effect_sub_title}}; 
  }
  .section-slideshow-v2 .slick-active .info-sideh2 .box-info-{{block.id}} .box-title-{{block.id}}{   
    -webkit-animation-name: {{block.settings.effect_title}};
    animation-name:  {{block.settings.effect_title}};
  }
  
  .section-slideshow-v2 .slick-active .info-sideh2 .box-info-{{block.id}} .box-sub-title2-{{block.id}}{    
    -webkit-animation-name: {{block.settings.effect_sub_title2}};
    animation-name:  {{block.settings.effect_sub_title2}}; 
  }
 
  .section-slideshow-v2 .slick-active .info-sideh2 .box-info-{{block.id}} .btn-animation-{{block.id}}{   
    -webkit-animation-name: {{block.settings.effect_button}};
    animation-name:  {{block.settings.effect_button}}; 
  }

  
  .section-slideshow-v2 .slick-active .info-sideh2 .box-info-{{block.id}} .box-sub-title-{{block.id}} .title-small{    
    color:{{block.settings.color_block_sub_title}};
    {% if block.settings.fonts_sub_title != blank %}
    font-family: '{{block.settings.fonts_sub_title}}';
      {% endif %}
  }
  .section-slideshow-v2 .slick-active .info-sideh2 .box-info-{{block.id}} .box-title-{{block.id}} .titlebig{   
    color:{{block.settings.color_block_title}};
    {% if block.settings.fonts_title != blank %}
    font-family: '{{block.settings.fonts_title}}';
      {% endif %}
  }
  .section-slideshow-v2 .slick-active .info-sideh2 .box-info-{{block.id}} .box-sub-title2-{{block.id}} .title-small{   
    color:{{block.settings.color_block_sub_title2}};
    {% if block.settings.fonts_sub_title2 != blank %}
    font-family: '{{block.settings.fonts_sub_title2}}';
      {% endif %}    
  }
  .section-slideshow-v2 .slick-active .info-sideh2 .box-info-{{block.id}} .btn-animation-{{block.id}} .button-main1{   
    {% if block.settings.fonts_button != blank %}
    font-family: '{{block.settings.fonts_button}}';
      {% endif %}
  }


  
  @media(min-width:1900px){
    .section-slideshow-v2 .slick-active .info-sideh2 .box-info-{{block.id}} .box-sub-title-{{block.id}} .title-small{    
      font-size:{{block.settings.font_size_sub_title}}px;
    }
    .section-slideshow-v2 .slick-active .info-sideh2 .box-info-{{block.id}} .box-title-{{block.id}} .titlebig{   
      font-size:{{block.settings.font_size_title}}px;
    }
    .section-slideshow-v2 .slick-active .info-sideh2 .box-info-{{block.id}} .box-sub-title2-{{block.id}} .title-small{   
      font-size:{{block.settings.font_size_sub_title2}}px; 
    }
    .section-slideshow-v2 .slick-active .info-sideh2 .box-info-{{block.id}} .btn-animation-{{block.id}} .button-main1{   
      font-size:{{block.settings.font_size_button}}px;
    }
  }
  
  
  {% endif %}
  {% endfor %}
  {% endif %}
</style>

